{
  "name": "semantic-ui-docs-gatsby",
  "private": true,
  "description": "Documentation for the Semantic UI component library.",
  "version": "0.2.0",
  "author": "Ryosuke <ryosuke.san.hana@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.1.1",
    "@mdx-js/react": "^1.1.1",
    "gatsby": "^5.9.0",
    "gatsby-image": "^2.2.7",
    "gatsby-plugin-less": "^2.1.2",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "^1.0.18",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-sharp": "^2.2.5",
    "less": "^3.9.0",
    "node-sass": "^4.12.0",
    "prism-react-renderer": "^0.1.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.0",
    "react-syntax-highlighter": "^11.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.3"
  },
  "devDependencies": {
    "html-loader": "^0.5.5",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "react",
    "gatsby",
    "mdx",
    "semantic-ui",
    "semantic-ui-react",
    "documentation"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && npm run develop",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --ext ts --ext tsx",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*",
      "content/*",
      "static/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whoisryosuke/semantic-ui-docs-gatsby"
  },
  "bugs": {
    "url": "https://github.com/whoisryosuke/semantic-ui-docs-gatsby/issues"
  }
}